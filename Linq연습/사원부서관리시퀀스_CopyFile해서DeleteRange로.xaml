    <Assign sap2010:Annotation.AnnotationText="투입일수 평균과 월지급액 그리고 자료없음입력" sap:VirtualizedContainerService.HintSize="485,114" sap2010:WorkflowViewState.IdRef="Assign_4">
      <Assign.To>
        <OutArgument x:TypeArguments="sd:DataTable">[outDT]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="sd:DataTable" xml:space="preserve">[(
From rowemp In EMPDT.AsEnumerable()
Group Join rowput In PUTDT.AsEnumerable()
On rowemp("사원번호").ToString Equals rowput("사원번호").ToString Into grp = Group
From g In grp.DefaultIfEmpty
Let str_a = CInt(If(IsNothing(rowemp("사원번호")), "", String.Format("{0:D6}", CInt(rowemp("사원번호")))))
Let a = If(IsNothing(g),0,CInt(g.ItemArray.Skip(1).Select(Function(x) CDec(x)).Average()))
Let put = If(a &lt;&gt; 0 AndAlso CInt(rowemp("투입일수")) = 0,a,CObj("자료없음"))
Let b = a*CInt(rowemp("일당"))
Let pay = If(a &lt;&gt; 0 AndAlso CInt(rowemp("일당")) &lt;&gt; 0,b,CObj("자료없음"))
Select outDT.Rows.Add({CObj(str_a)}.Concat(rowemp.ItemArray.Skip(1).Take(3)).Concat({CObj(put)}).Concat(rowemp.ItemArray.Skip(5).Take(1)).Concat({CObj(pay)}).ToArray())
).CopyToDataTable]</InArgument>
      </Assign.Value>
      <sap:WorkflowViewStateService.ViewState>
        <scg:Dictionary x:TypeArguments="x:String, x:Object">
          <x:Boolean x:Key="IsAnnotationDocked">True</x:Boolean>
        </scg:Dictionary>
      </sap:WorkflowViewStateService.ViewState>
    </Assign>
